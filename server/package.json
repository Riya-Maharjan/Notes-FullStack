{
  "name": "note-api",
  "description": "API service for CRUD and login / register operations",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "start:dev": " nodemon --watch  src/ src/index.js",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "rollback": "knex migrate:rollback --knexfile src/knexfile.js",
    "migrate": "knex migrate:latest --knexfile src/knexfile.js",
    "seed": "knex seed:run --knexfile src/knexfile.js",
    "seed:create": "knex seed:make --knexfile src/knexfile.js",
    "migrate:create": "knex migrate:make --knexfile src/knexfile.js",
    "prettify": "prettier --single-quote --print-width 120 --write \"src/**/*.js\""
  },
  "dependencies": {
    "@hapi/boom": "^9.1.4",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.1",
    "camelize": "^1.0.0",
    "cors": "^2.8.5",
    "diskdb": "^0.1.17",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "express-jwt": "^6.1.1",
    "helmet": "^5.0.2",
    "http-status-codes": "^2.2.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^1.0.4",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.15",
    "pg": "^8.7.3",
    "serve-favicon": "^2.5.0",
    "snakeize": "^0.1.0",
    "winston": "^3.5.1"
  },
  "devDependencies": {
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.1"
  }
}